<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.hungrysnake">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LevelClass"
            android:label="LEVELS"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.LEVELCLASS" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FairIntentClass"
            android:label="HUNGRY SNAKE"
            android:theme="@style/AppTheme">>
            <intent-filter>
                <action android:name="android.intent.action.FAIRINTENTCLASS" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".IfHighScore"
            android:label="HUNGRY SNAKE"
            android:theme="@style/AppTheme">>
            <intent-filter>
                <action android:name="android.intent.action.IFHIGHSCORE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HighScore"
            android:label="HUNGRY SNAKE"
            android:theme="@style/AppTheme">>
            <intent-filter>
                <action android:name="android.intent.action.HIGHSCORE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Help"
            android:label="HUNGRY SNAKE"
            android:theme="@style/AppTheme">>
            <intent-filter>
                <action android:name="android.intent.action.HELP" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
